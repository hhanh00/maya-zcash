[Error]
enum ZcashError {
    "RPC",
    "InvalidPubkeyLength",
};

dictionary Height {
    u32 number;
    bytes hash;
};

namespace maya_zcash {
    [Throws=ZcashError]
    Height get_latest_height();

    [Throws=ZcashError]
    string get_vault_address(bytes pubkey);

    [Throws=ZcashError]
    boolean validate_address(string address);
};
